{
  "Environment": "Production",
  "Cors": {
    "AllowedOrigins": [
      "https://app.intellifin.africa",
      "https://admin.intellifin.africa"
    ]
  },
  "RabbitMq": {
    "Host": "rabbitmq.intellifin.internal",
    "Port": 5671,
    "VirtualHost": "/",
    "Username": "credit_assessment",
    "Password": "CHANGE_ME"
  },
  "VaultRules": {
    "Address": "https://vault.intellifin.africa",
    "Enabled": true
  },
  "TransUnion": {
    "BaseUrl": "https://api.transunionafrica.com",
    "Timeout": "00:00:15",
    "RetryCount": 5
  },
  "Pmec": {
    "BaseUrl": "https://pmec.gov.zm/api",
    "Timeout": "00:00:15",
    "RetryCount": 5,
    "ApiKey": "CHANGE_ME"
  },
  "ClientManagement": {
    "BaseUrl": "https://client-management.intellifin.internal",
    "Timeout": "00:00:10",
    "RetryCount": 3
  },
  "Zeebe": {
    "GatewayAddress": "zeebe.intellifin.internal:26500",
    "Audience": "zeebe-api",
    "ClientId": "credit-assessment-worker",
    "ClientSecret": "CHANGE_ME",
    "AuthorizationServerUrl": "https://identity.intellifin.africa/realms/zeebe/protocol/openid-connect/token"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
