<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.0.0">
  <bpmn:process id="loanOriginationProcess" name="Loan Origination Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Loan Application Initiated">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Activity_KycGate" />
    
    <!-- Service Task: KYC Gate -->
    <bpmn:serviceTask id="Activity_KycGate" name="KYC Gate">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="verify-kyc" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Activity_KycGate" targetRef="Activity_ApplicationReview" />
    
    <!-- User Task: Application Review -->
    <bpmn:userTask id="Activity_ApplicationReview" name="Review Loan Application">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="loan-officers" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Activity_ApplicationReview" targetRef="Activity_RequestCreditAssessment" />
    
    <!-- Service Task: Credit Assessment -->
    <bpmn:serviceTask id="Activity_RequestCreditAssessment" name="Credit Assessment">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="request-credit-assessment" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Activity_RequestCreditAssessment" targetRef="Gateway_ApprovalRouting" />
    
    <!-- Exclusive Gateway: Approval Routing -->
    <bpmn:exclusiveGateway id="Gateway_ApprovalRouting" name="Approval Routing">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCreditAnalyst</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToHeadOfCredit</bpmn:outgoing>
      <bpmn:outgoing>Flow_ToDualControl</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <!-- Route 1: Credit Analyst (Low risk, low amount) -->
    <bpmn:sequenceFlow id="Flow_ToCreditAnalyst" name="Low Risk &#38; Low Amount" sourceRef="Gateway_ApprovalRouting" targetRef="Activity_CreditAnalystApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=loanAmount &lt;= 50000 and riskGrade in ["A", "B"]</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_CreditAnalystApproval" name="Credit Analyst Approval">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="credit-analysts" excludeUsers="#{createdBy},#{assessedBy}" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToCreditAnalyst</bpmn:incoming>
      <bpmn:outgoing>Flow_FromCreditAnalyst</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_FromCreditAnalyst" sourceRef="Activity_CreditAnalystApproval" targetRef="Gateway_ApprovalMerge" />
    
    <!-- Route 2: Head of Credit (Medium risk or medium amount) -->
    <bpmn:sequenceFlow id="Flow_ToHeadOfCredit" name="Medium Risk or Amount" sourceRef="Gateway_ApprovalRouting" targetRef="Activity_HeadOfCreditApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=(loanAmount &gt; 50000 and loanAmount &lt;= 250000) or riskGrade = "C"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_HeadOfCreditApproval" name="Head of Credit Approval">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="head-of-credit" excludeUsers="#{createdBy},#{assessedBy}" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToHeadOfCredit</bpmn:incoming>
      <bpmn:outgoing>Flow_FromHeadOfCredit</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_FromHeadOfCredit" sourceRef="Activity_HeadOfCreditApproval" targetRef="Gateway_ApprovalMerge" />
    
    <!-- Route 3: Dual Control (High risk or high amount) -->
    <bpmn:sequenceFlow id="Flow_ToDualControl" name="High Risk or Amount" sourceRef="Gateway_ApprovalRouting" targetRef="Activity_HeadOfCreditApproval_DualControl">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=loanAmount &gt; 250000 or riskGrade in ["D", "F"]</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_HeadOfCreditApproval_DualControl" name="Head of Credit Approval (First)">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="head-of-credit" excludeUsers="#{createdBy},#{assessedBy}" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToDualControl</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCeoApproval</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_ToCeoApproval" sourceRef="Activity_HeadOfCreditApproval_DualControl" targetRef="Activity_CeoApproval" />
    <bpmn:userTask id="Activity_CeoApproval" name="CEO Approval (Second)">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="ceo" excludeUsers="#{createdBy},#{assessedBy},#{firstApproverId}" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToCeoApproval</bpmn:incoming>
      <bpmn:outgoing>Flow_FromCeoApproval</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_FromCeoApproval" sourceRef="Activity_CeoApproval" targetRef="Gateway_ApprovalMerge" />
    
    <!-- Merge Gateway: All approval paths converge -->
    <bpmn:exclusiveGateway id="Gateway_ApprovalMerge" name="Approval Complete">
      <bpmn:incoming>Flow_FromCreditAnalyst</bpmn:incoming>
      <bpmn:incoming>Flow_FromHeadOfCredit</bpmn:incoming>
      <bpmn:incoming>Flow_FromCeoApproval</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_ApprovalMerge" targetRef="Activity_GenerateAgreement" />
    
    <!-- Service Task: Agreement Generation -->
    <bpmn:serviceTask id="Activity_GenerateAgreement" name="Generate Agreement">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-agreement" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Activity_GenerateAgreement" targetRef="Activity_DisbursementAuthorization" />
    
    <!-- User Task: Disbursement Authorization -->
    <bpmn:userTask id="Activity_DisbursementAuthorization" name="Authorize Disbursement">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="finance-officers" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Activity_DisbursementAuthorization" targetRef="Activity_ExecuteDisbursement" />
    
    <!-- Service Task: Execute Disbursement -->
    <bpmn:serviceTask id="Activity_ExecuteDisbursement" name="Execute Disbursement">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="execute-disbursement" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Activity_ExecuteDisbursement" targetRef="EndEvent_1" />
    
    <bpmn:endEvent id="EndEvent_1" name="Loan Active">
      <bpmn:incoming>Flow_8</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
  
